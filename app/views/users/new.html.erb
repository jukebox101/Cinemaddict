<h1>Create new User</h1>

<%= form_for @user do |f| %>
  <%= f.label :username %>:
  <%= f.text_field :username %>
  <br>
  <br>
  <%= f.label :first_name %>
  <%= f.text_field :first_name %>
  <br>
  <br>
  <%= f.label :last_name %>
  <%= f.text_field :last_name %>
  <br>
  <br>
  <%= f.label :password %>
  <%= f.text_field :password %>
  <br>
  <br>
  <%= f.label :email %>
  <%= f.email_field :email %>
  <br>
  <br>
  <%= f.submit %>
<% end %>


